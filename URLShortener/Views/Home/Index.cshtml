@model List<URL>

@{
    ViewData["Title"] = "Home Page";
}

<div>
    @using (Html.BeginForm("Create", "Home", FormMethod.Post))
    {
        <input type="text" name="Address" placeholder="Address" /> <br/>
        <button type="submit">Add</button>
    }
</div>
<div>
    <table>
        @foreach (var item in Model)
    {
        <tr>
            <td><strong>@Html.DisplayNameFor(model => item.Address):</strong></td>
            <td>@Html.DisplayFor(model => item.Address),</td>
            <td><strong>@Html.DisplayNameFor(model => item.ShortAdress):</strong></td>
            <td>@Html.DisplayFor(model => item.ShortAdress)</td>
        </tr>



}
    </table>

</div>
